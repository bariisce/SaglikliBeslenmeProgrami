{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "TokenOptions": {
    "Issuer": "your-issuer",
    "Audience": "your-audience",
    "SecurityKey": "your-secret-key" // Bu anahtar en az 16 karakter uzunluğunda olmalıdır
  }
}